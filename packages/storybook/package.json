{
    "name": "@ui-kit.ai/storybook",
    "version": "0.0.0-alpha.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/alex-mcgovern/ui-kit.ai"
    },
    "license": "MIT",
    "author": "Alex McGovern",
    "type": "module",
    "main": "dist/index.js",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "check:eslint": "eslint .",
        "check:prettier": "prettier . --check",
        "check:tsc": "tsc",
        "check:vitest": "vitest run --passWithNoTests",
        "fix:eslint": "eslint . --fix",
        "fix:prettier": "prettier . --write",
        "storybook": "storybook dev -p 65534",
        "storybook:docs": "storybook dev --docs -p 65534",
        "storycap": "storycap --serverCmd \"storybook dev -p 65534\" http://localhost:65534 --include \"**/Primary\" --flat --outDir \"assets\""
    },
    "dependencies": {
        "@mdx-js/react": "3.1.0",
        "@tanstack/react-query": "5.66.0",
        "@ui-kit.ai/components": "workspace:*",
        "lucide-react": "0.474.0",
        "postcss": "8.4.47",
        "react": "19.0.0",
        "react-aria-components": "1.7.1",
        "react-dom": "19.0.0",
        "tailwind-merge": "2.5.2",
        "tailwindcss": "3.4.17",
        "zod": "3.24.1"
    },
    "devDependencies": {
        "@faker-js/faker": "8.4.1",
        "@hookform/resolvers": "4.1.2",
        "@storybook/addon-essentials": "8.6.6",
        "@storybook/addon-interactions": "8.6.6",
        "@storybook/addon-links": "8.6.6",
        "@storybook/addon-onboarding": "8.6.6",
        "@storybook/addon-themes": "8.6.6",
        "@storybook/blocks": "8.6.6",
        "@storybook/builder-vite": "8.6.6",
        "@storybook/manager-api": "8.6.6",
        "@storybook/react": "8.6.6",
        "@storybook/react-vite": "8.6.6",
        "@storybook/theming": "8.6.6",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.2.0",
        "@testing-library/user-event": "14.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.13.10",
        "@types/react": "19.0.12",
        "@types/react-dom": "19.0.4",
        "@ui-kit.ai/eslint": "workspace:*",
        "@ui-kit.ai/mocks": "workspace:*",
        "@ui-kit.ai/prettier": "workspace:*",
        "@ui-kit.ai/tsconfig": "workspace:*",
        "@ui-kit.ai/vite": "workspace:*",
        "@ui-kit.ai/vitest": "workspace:*",
        "autoprefixer": "10.4.20",
