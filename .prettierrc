{
    "endingPosition": "relative",
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-classnames",
        "prettier-plugin-merge"
    ],
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "tailwindConfig": "./tailwind.config.ts",
    "trailingComma": "es5",
    "useTabs": false,
    "tailwindFunctions": [
        "composeTailwindRenderProps",
        "tv",
        "twMerge",
        "composeTailwindRenderProps"
    ],
    "customFunctions": [
        "composeTailwindRenderProps",
        "tv",
        "twMerge",
        "composeTailwindRenderProps"
    ]
}
